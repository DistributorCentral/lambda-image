{
	"author": {
		"email": "prefinem@gmail.com",
		"name": "Prefinem",
		"url": "https://prefinem.com"
	},
	"description": "Lambda Image Class for easy image manipulation on AWS Lambda",
	"peerDependencies": {
		"aws-sdk": ">= 2.x",
		"got": ">= 9.x"
	},
	"devDependencies": {
		"@babel/cli": "^7.1.2",
		"@babel/core": "^7.1.2",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0",
		"@babel/preset-env": "^7.1.0",
		"babel-eslint": "^10.0.1",
		"eslint": "^5.7.0",
		"eslint-config-prettier": "^3.1.0",
		"eslint-plugin-babel": "^5.2.1",
		"eslint-plugin-jest": "^21.26.0",
		"husky": "^1.1.2",
		"jest": "^23.6.0",
		"lint-staged": "^7.3.0",
		"prettier": "^1.14.3"
	},
	"homepage": "https://github.com/Prefinem/lambda-image",
	"husky": {
		"hooks": {
			"pre-commit": "yarn staged"
		}
	},
	"license": "MIT",
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"prettier --config ./.prettierrc.json --write",
			"jest --bail --findRelatedTests",
			"git add"
		]
	},
	"main": "index.js",
	"name": "lambda-image",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Prefinem/lambda-image"
	},
	"scripts": {
		"build": "./node_modules/.bin/babel src/ --out-dir dist",
		"clean": "yarn lint && yarn prettify",
		"eslint-check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
		"lint": "eslint src/* --fix",
		"prettify": "prettier --config ./.prettierrc.json --write \"src/**\"",
		"staged": "lint-staged",
		"test": "jest --coverage"
	},
	"version": "1.0.0",
	"dependencies": {
		"gm": "^1.23.1"
	}
}

{
	"author": {
		"email": "prefinem@gmail.com",
		"name": "Prefinem",
		"url": "https://prefinem.com"
	},
	"dependencies": {
		"gm": "^1.23.1"
	},
	"description": "Lambda Image Class for easy image manipulation on AWS Lambda",
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.2.2",
		"@babel/plugin-proposal-object-rest-spread": "^7.2.0",
		"@babel/preset-env": "^7.2.3",
		"babel-eslint": "^10.0.1",
		"eslint": "^5.12.0",
		"eslint-config-prettier": "^3.5.0",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-jest": "^21.27.2",
		"fs-extra": "^7.0.1",
		"husky": "^1.3.1",
		"jest": "^23.6.0",
		"lint-staged": "^7.3.0",
		"prettier": "^1.15.3"
	},
	"homepage": "https://github.com/Prefinem/lambda-image",
	"husky": {
		"hooks": {
			"pre-commit": "yarn staged"
		}
	},
	"license": "MIT",
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"prettier --config ./.prettierrc.json --write",
			"jest --bail --findRelatedTests",
			"git add"
		]
	},
	"main": "index.js",
	"name": "lambda-image",
	"peerDependencies": {
		"aws-sdk": "^2.348.0",
		"got": "^9.3.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Prefinem/lambda-image"
	},
	"scripts": {
		"build": "./node_modules/.bin/babel src/ --out-dir dist",
		"clean": "yarn lint && yarn prettify",
		"eslint-check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
		"lint": "eslint src/* --fix",
		"prettify": "prettier --config ./.prettierrc.json --write \"src/**\"",
		"staged": "lint-staged",
		"test": "yarn add got aws-sdk -P && jest --coverage"
	},
	"version": "1.0.3"
}
